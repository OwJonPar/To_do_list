<p id="notice"><%= notice %></p>

<h1>Listing To Do</h1>

<table>
 <thead>
   <tr>
     <th>To Do Item</th>
     <th colspan="3"></th>
   </tr>
 </thead>

 <tbody>
   <% @to_dos.each do |t| %>
     <tr>
       <td><%= t.text %></td>
       <td><%= link_to 'Show', t %></td>
       <td><%= link_to 'Edit', edit_to_do_path(t) %></td>
       <td><%= link_to 'Destroy', t, method: :delete, data: { confirm: 'Are you sure?' } %></td>
     </tr>
   <% end %>
 </tbody>
</table>

<br>

<%= link_to 'New To Do', new_to_do_path %>
